{! This car should only be used in the pagination navigation bar.  !}
<li {?data.active}class="active"{/data.active} data-number="{data.number}">
  {^data.active}<a href="{data.url}">{/data.active}
  {?data.active}<span>{/data.active}
    <span>{@t key="common.number-navigation.page" /}</span>
    <span class="count">{data.number}</span>
    <span>{@t key="{data.total-count}" /}</span>
  {?data.active}</span>{/data.active}
  {^data.active}</a>{/data.active}
</li>
